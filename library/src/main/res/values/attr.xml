<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CodoEditText">
        <attr name="funtype" format="enum">
            <enum name="normal" value="-1"></enum>
            <enum name="clear" value="0"></enum>
            <enum name="pwdExplode" value="1"></enum>
            <enum name="search" value="2"></enum>
        </attr>
        <attr name="eyeOpen" format="reference"></attr>
        <attr name="eyeClose" format="reference"></attr>
        <!--左侧Drawable宽度-->
        <attr name="leftDrawableWidth" format="dimension"/>
        <!--左侧Drawable的高度-->
        <attr name="leftDrawableHeight" format="dimension"/>
        <!--右侧Drawable的宽度-->
        <attr name="rightDrawableWidth" format="dimension"/>
        <!--右侧Drawable的高度-->
        <attr name="rightDrawableHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CodoTextView">
        <attr name="leftDrawableTop" format="dimension"></attr>
        <attr name="leftDrawableLeft" format="dimension"></attr>
        <attr name="textviewleftDrawableWidth" format="dimension"></attr>
        <attr name="textviewrightDrawableWidth" format="dimension"></attr>
        <attr name="textviewleftDrawableHeight" format="dimension"></attr>
        <attr name="textviewrightDrawableHeight" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="CodoIMEditText">
    </declare-styleable>
</resources>